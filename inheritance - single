1.class Single {
    public static void main(String[] args) {
        son s = new son();  
        System.out.println(s.gender);
        s.print();          
    }
}

class father {
    char gender = 'M';
    void print() {
        System.out.println("parent class");
    }
}

// subclass inheriting father
class son extends father {
    
}
