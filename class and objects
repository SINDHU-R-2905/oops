1.application form:

public class appform{
String name;
int rollno;
public void display (){
System.out.println(name);
System.out.println(rollno);
}
public static void main(String[] args) {
appform codeio = new appform();
codeio.name = "codeio";
codeio.rollno = 71;
codeio.display();
}
}

2.creating objects:

class applform {
String name;
int rollno;

public void display () {
    System.out.println(name);
    System.out.println(rollno);
}
}

public class Main {
public static void main(String[] args) {
applform codeio = new applform();
codeio.name = "codeio";
codeio.rollno = 71;
codeio.display();

    applform viewer = new applform();
    viewer.name = "viewer";
    viewer.rollno = 78106;
    viewer.display();
}
}


3.class ApplForm {
    String name;
    int rollno;

    public void display() {
        System.out.println(name);
        System.out.println(rollno);
    }

    public void setvalues(String str, int num) {
        name = str;
        rollno = num;
    }
}

public class demo1 {   
    public static void main(String[] args) {
        ApplForm sindhu = new ApplForm();
        sindhu.setvalues("sindhu", 71);
        sindhu.display();

        ApplForm viewer = new ApplForm();
        viewer.setvalues("viewer", 78106);
        viewer.display();
    }
}

